{
  "name": "@centrality-checker/chrome-extension",
  "version": "1.0.0",
  "description": "A chrome extension to see the centrality of npm packages and check if a package is in decline.",
  "main": "index.js",
  "sideEffects": false,
  "dependencies": {
    "apexcharts": "^3.24.0",
    "date-fns": "^2.16.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.129",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "hook-shell-script-webpack-plugin": "^0.1.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.20.1",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "webpack --config webpack/webpack.dev.js --watch",
